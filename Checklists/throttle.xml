<?xml version="1.0"?>
<!-- Throttle check -->
<PropertyList>
		<condition>
			<and>
				<greater-than-equals>
					<property>/controls/engines/engine/throttle</property>
					<value alias="../../../../min-throttle"/>
				</greater-than-equals>
				<less-than-equals>
					<property>/controls/engines/engine/throttle</property>
					<value alias="../../../../max-throttle"/>
				</less-than-equals>
			</and>
		</condition>
</PropertyList>
