<?xml version="1.0"?>
<!-- Manifold Pressure check -->
<PropertyList>
	<condition>
		<and>
			<greater-than>
				<property>/engines/engine/mp-inhg</property>
				<value alias="../../../../min-mp-inhg"/>
			</greater-than>
			<less-than>
				<property>/engines/engine/mp-inhg</property>
				<value alias="../../../../max-mp-inhg"/>
			</less-than>
		</and>
	</condition>
	<marker>
    <x-m>2.625</x-m>
    <y-m>0.2092</y-m>
    <z-m>0.1949</z-m>
    <scale>2.7833</scale>
	</marker>
</PropertyList>
