    <checklist>
        <title>Climb</title>
        <item include="manifold.xml">
            <name>Manifold Pressure</name>
            <value>46 inHG</value>
            <min-mp-inhg>45</min-mp-inhg>
            <max-mp-inhg>47</max-mp-inhg>
        </item>
        <item include="rpm.xml">
            <name>Propeller Pitch</name>
            <value>2700RPM</value>
            <min-rpm>2650</min-rpm>
            <max-rpm>2700</max-rpm>
        </item>
        <item include="mixture-run.xml">
            <name>Mixture</name>
            <value>RUN</value>
        </item>
        <item include="supercharger-auto.xml">
            <name>Supercharger</name>
            <value>AUTO</value>
        </item>
        <item>
            <name>Throttle automatic boost control will hold the manifold pressure</name>
            <value/>
        </item>
        <item>
            <name>setting as long as the supercharger has enough boost.</name>
            <value/>
        </item>
        <item>
            <name>It will hold 46 inHG to about 30,000ft.</name>
            <value/>
        </item>
    </checklist>
    <checklist>
        <title>Pre-traffic pattern check</title>
        <item>
            <name>Fuel selector</name>
            <value>Fullest internal tank</value>
        </item>
        <item include="fuel-boost-on.xml">
            <name>Fuel Boost</name>
            <value>ON</value>
        </item>
        <item include="carburetor.xml">
            <name>Carburetor RAM / HOT-AIR</name>
            <value>As needed</value>
        </item>
        <item include="mixture-run.xml">
            <name>Mixture</name>
            <value>RUN</value>
        </item>
        <item include="rpm.xml">
            <name>Propeller Pitch</name>
            <value>2700RPM</value>
            <min-rpm>2650</min-rpm>
            <max-rpm>2700</max-rpm>
        </item>
        <item>
            <name>Oil and coolant radiators</name>
            <value>AUTOMATIC</value>
        </item>
        <item>
            <name>Clean out engine at 3000RPM and 60inHG for one minute.</name>
        </item>
    </checklist>

    <checklist>
        <title>Traffic pattern check</title>
        <item>
            <name>Landing Gear</name>
            <value>DOWN (below 170 mph)</value>
            <condition>
                <property>/controls/gear/gear-down</property>
                <value>true</value>
            </condition>
            <value>Check gear position by use of warning lights, horn and hydraulic pressure</value>
        </item>
        <item include="flaps.xml">
            <name>Flaps</name>
            <value>15 (if desired)</value>
            <flaps>0.2</flaps>
        </item>
        <item>
            <name>Recheck gear and flaps</name>
            <value/>
        </item>
        <item>
            <name>Throttle closed when landing assured</name>
            <value/>
        </item>
        <item include="flaps.xml">
            <name>Flaps</name>
            <value>DOWN (above 400ft)</value>
            <flaps>1</flaps>
        </item>
        <item>
            <name>Speed</name>
            <value>120 IAS at edge of field</value>
        </item>
    </checklist>
