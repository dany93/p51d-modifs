    <PropertyList>
    <checklist>
        <title>Engine Start</title>
        <item include="prop-full.xml">
            <name>Prop</name>
            <value>FULL INCREASE</value>
        </item>
        <item include="magneto-off.xml">
            <name>Ignition switch</name>
            <value>OFF</value>
        </item>
        <item include="battery-not-connected.xml">
            <name>Battery Disconnect Off</name>
            <value>OFF</value>
        </item>
        <item include="mixture-idle.xml">
            <name>Mixture</name>
            <value>IDLE CUTOFF</value>
        </item>
        <item>
            <name>Ground Crew Propeller Pull Through</name>
            <value>Not Modeled Skip</value>
        </item>
        <item include="fuel-on.xml">
            <name>Fuel Cutoff</name>
            <value>ON</value>
        </item>
        <item include="battery-connected.xml">
            <name>Battery disconnect</name>
            <value>ON</value>
        </item>
        <item include="gunsight-on.xml">
            <name>Gunsight</name>
            <value>ON</value>
        </item>
        <item include="gunsight-gyro-on.xml">
            <name>Gunsight Gyro</name>
            <value>ON</value>
        </item>	
        <item include="throttle.xml">
            <name>Throttle</name>
            <value>ONE INCH</value>
            <min-throttle>0.14</min-throttle>
            <max-throttle>0.18</max-throttle>
        </item>
        <item>
            <name>Prop</name>
            <value>CLEAR</value>
        </item>
        <item include="starter-on.xml">
            <name>Start Switch</name>
            <value>ON</value>
        </item>
        <item include="ignition-both.xml">
            <name>Ignition</name>
            <value>BOTH (after 6 blades)</value>
        </item>
        <item include="fuel-boost-on.xml">
            <name>Fuel Boost</name>
            <value>ON</value>
        </item>
        <item include="primer-on.xml">
            <name>Primer</name>
            <value>ON</value>
        </item>
        <item include="starter-on.xml">
            <name>Start Switch</name>
            <value>ON</value>
        </item>
        <item include="mixture-run.xml">
            <name>Mixture (after engine start)</name>
            <value>RUN</value>
        </item>
        <item include="primer-off.xml">
            <name>Primer</name>
            <value>OFF</value>
        </item>
        <item include="oil-pressure.xml">
            <name>Oil Pressure</name>
            <value>50 psi</value>
        </item>	
        <item include="generator-on.xml">
            <name>Generator Disconnect</name>
            <value>ON</value>
        </item>
    </checklist>
    <checklist>
        <title>Pre-flight Check</title>
        <item>
            <name>Primary Controls</name>
            <value>Check for free movement</value>
        </item>
        <item>
            <name>Altimeter</name>
            <value>SET</value>
        </item>
        <item>
            <name>Directional Gyro</name>
            <value>SET</value>
        </item>
        <item>
            <name>Instruments</name>
            <value>Correct</value>
        </item>
        <item>
            <name>Switches</name>
            <value>Correct position</value>
        </item>
        <item include="fuel-selector.xml">
            <name>Fuel tank Selector</name>
            <value>Main LH</value>
        </item>
        <item include="fuel-boost-on.xml">
            <name>Fuel Boost</name>
            <value>ON</value>
        </item>
        <item include="primer-off.xml">
            <name>Primer</name>
            <value>OFF</value>
        </item>
        <item include="flaps-up.xml">
            <name>Flaps</name>
            <value>UP (normal take-off)</value>
        </item>
        <item><name>Fuselage tank 0-25 GAL</name></item>
        <item include="trim-rudder.xml">
            <name>Rudder Trim</name>
            <value>6 degrees right</value>
            <min-trim>0.45</min-trim>
            <max-trim>0.55</max-trim>
        </item>
        <item include="trim-aileron.xml">
            <name>Aileron Trim</name>
            <value>0 degrees</value>
            <min-trim>-0.05</min-trim>
            <max-trim>0.05</max-trim>
        </item>
        <item include="trim-elevator.xml">
            <name>Elevator Trim</name>
            <value>0 degrees</value>
            <min-trim>-0.05</min-trim>
            <max-trim>0.05</max-trim>
        </item>
        <item><name>Fuselage tank full</name></item>
        <item include="trim-rudder.xml">
            <name>Rudder Trim</name>
            <value>6 degrees right</value>
            <min-trim>0.45</min-trim>
            <max-trim>0.55</max-trim>
        </item>
        <item include="trim-aileron.xml">
            <name>Aileron Trim</name>
            <value>0 degress</value>
            <min-trim>-0.05</min-trim>
            <max-trim>0.05</max-trim>
        </item>
        <item include="trim-elevator.xml">
            <name>Elevator Trim</name>
            <value>0-4 degrees nose heavy</value>
            <min-trim>0.3</min-trim>
            <max-trim>0.5</max-trim>
        </item>
        <item>
            <name>Circuit Breakers</name>
            <value>ALL IN</value>
        </item>
        <item>
            <name>Canopy</name>
            <value>LOCKED</value>
            <value>EMERGENCY RELEASE HANDLE SAFETIED</value>
        </item>
    </checklist>
    <checklist>
        <title>Engine Check</title>
        <item include="prop-full.xml">
            <name>Prop</name>
            <value>FULL INCREASE</value>
        </item>
        <item include="rpm.xml">
            <name>Power</name>
            <value>Advance throttle to 2300RPM</value>
            <value>Manifold pressure matches field barometric pressure +-1/2 in Hg</value>
            <min-rpm>2250</min-rpm>
            <max-rpm>2350</max-rpm>
        </item>
        <item>
            <name>Magneto check</name>
            <value>Max 100RPM drop right</value>
            <value>Max 120RPM drop left</value>
        </item>
        <item>
            <name>Idle speed check</name>
            <value>650-700rpm with throttle at idle stop</value>
        </item>
        <item>
            <name>Acceleration/deceleration check</name>
            <value>Throttle idle - 2300RPM</value>
            <value>Engine accelerates and decelerates smoothly, no backfire</value>
        </item>
        <item include="rpm.xml">
            <name>Throttle for best cooling</name>
            <value>1500RPM </value>
            <min-rpm>1400</min-rpm>
            <max-rpm>1600</max-rpm>
        </item>
        <item include="carburetor.xml">
            <name>Carburetor ram-air</name>
            <value>RAM AIR</value>
        </item>
        <item include="mixture-run.xml">
            <name>Mixture</name>
            <value>RUN</value>
        </item>
        <item include="supercharger-auto.xml">
            <name>Supercharger</name>
            <value>AUTO</value>
        </item>
        <item>
            <name>Oil and coolant radiators</name>
            <value>AUTOMATIC</value>
        </item>
    </checklist>
