<?xml version="1.0"?>
<!-- Mixture Idle check -->
<PropertyList>
	<condition>
		<and>
			<greater-than>
				<property>/controls/engines/engine/mixture</property>
				<value>0.1</value>
			</greater-than>
			<less-than>
				<property>/controls/engines/engine/mixture</property>
				<value>0.9</value>
			</less-than>
		</and>
	</condition>
	<marker>
    <x-m>2.82</x-m>
    <y-m>-0.3</y-m>
    <z-m>0.07</z-m>
    <scale>2</scale>
	</marker>
</PropertyList>
