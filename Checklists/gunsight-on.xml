<?xml version="1.0"?>
<!-- Gunsight gyro on check -->
<PropertyList>
	<condition>
		<equals>
			<property>/controls/armament/gunsight/power-on</property>
			<value>1</value>
		</equals>
	</condition>
	<marker>
		<x-m>2.645</x-m>
		<y-m>-0.09</y-m>
		<z-m>-0.06</z-m>
		<scale>1</scale>
	</marker>
</PropertyList>