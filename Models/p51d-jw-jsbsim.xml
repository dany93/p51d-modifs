<!-- JSBSIm FDM by Hal V. Engel -->

<PropertyList include="p51d-bombableinclude.xml">

 <path>P-51D-25NA.ac</path>
 
  <nasal>
    <load>
      var self = cmdarg();
	  print("LOAD P-51D-25NA ", self.getPath());
      var livery_update = aircraft.livery_update.new("Aircraft/p51d/Models/Liveries", 10);
    </load>
 
    <unload>
      print("UNLOAD P-51D-25NA ", self.getPath());
      livery_update.stop();
    </unload>
 </nasal>

     <animation>
        <type>range</type>
        <min-m>0</min-m>
        <max-m>30000</max-m>
    </animation>

 <!-- Pilot -->
 <model>
   <path>Aircraft/p51d/Models/Pilot/pilotWWII.xml</path>
   <name>pilot</name>
   <nopreview/>
   <offsets>
     <x-m>2.99</x-m>
     <y-m>-0.0</y-m>
     <z-m>0.215</z-m>
   </offsets>
 </model>
 
