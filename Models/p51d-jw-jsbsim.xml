  </offsets>
 </model> 

 <animation>
   <type>select</type>
   <object-name>pilot</object-name>
   <condition>
   <or>
       <greater-than>
         <property>/sim/current-view/view-number</property>
         <value>0.0</value>
       </greater-than>
       <greater-than>
         <property>/fdm/jsbsim/systems/pilotAlwaysVisible</property>
         <value>0.0</value>
       </greater-than>
       <!-- so the pilot is always visible on multiplayer -->
       <not-equals>
         <property>/sim/aircraft</property>
         <value>p51d-jsbsim</value>
       </not-equals>           
     </or>
   </condition>
 </animation> 

 <animation>
  <type>material</type>
  <object-name>pilot</object-name>
  <emission>
    <factor-prop>controls/lighting/cabin-norm</factor-prop>
    <red>0.25</red>
    <green>0.15</green>
    <blue>0.15</blue>
  </emission>
 </animation>
 
 
