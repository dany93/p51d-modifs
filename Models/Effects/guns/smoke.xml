<?xml version="1.0" encoding="UTF-8"?>

<PropertyList>

  <path>puff.ac</path>

  <animation>
    <enable-hot type="bool">false</enable-hot>
  </animation>

  <animation>
    <type>billboard</type>
    <spherical>true</spherical>
  </animation>

  <animation>
    <type>scale</type>
    <object-name>puff</object-name>
    <use-personality>true</use-personality>
    <property>sim/time/elapsed-sec</property>
    <x-min>    1.0 </x-min>
    <y-min>    1.0 </y-min>
    <z-min>    1.0 </z-min>
    <x-factor> 2.0 </x-factor>
    <y-factor> 2.0 </y-factor>
    <z-factor> 2.0 </z-factor>
  </animation>

  <animation>
    <type>blend</type>
    <object-name>puff</object-name>
    <use-personality>true</use-personality>
    <property>sim/time/elapsed-sec</property>
    <factor> 0.01 </factor>
    <offset> 0.0 </offset>
    <min>    0.3 </min>
    <max>    1.0 </max>
  </animation>
</PropertyList>
