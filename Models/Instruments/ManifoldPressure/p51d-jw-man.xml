<?xml version="1.0"?>
<PropertyList>

 <path>p51d-jw-man.ac</path>

 <animation>
  <type>material</type>
  <object-name>Face</object-name>
  <object-name>Needle1</object-name>
  <emission>
    <factor-prop>controls/lighting/instruments-norm</factor-prop>
    <red>0.35</red>
    <green>0.35</green>
    <blue>0.4</blue>
  </emission>
 </animation>

 <animation>
  <type>material</type>
  <object-name>bezeloutside</object-name>
  <object-name>bezelBase</object-name>
  <emission>
    <factor-prop>/controls/lighting/cabin-norm</factor-prop>
    <red>0.2</red>
    <green>0.1</green>
    <blue>0.1</blue>
  </emission>
 </animation>

 <animation>
  <type>rotate</type>
  <object-name>Needle1</object-name>
  <property>engines/engine/mp-osi</property>
  <min>15</min>
  <max>100</max>
  <interpolation>
   <entry>
    <ind>15</ind>
    <dep>0</dep>
   </entry>
   <entry>
    <ind>30</ind>
    <dep>60</dep>
   </entry>
   <entry>
    <ind>100</ind>
    <dep>350</dep>
   </entry>
  </interpolation>
  <axis>
   <x>-1</x>
   <y>0</y>
   <z>0</z>
  </axis>
 </animation>

</PropertyList>


