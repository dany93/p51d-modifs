<?xml version="1.0"?>
<PropertyList>

 <path>p51d-jw-vac.ac</path>

 <animation>
  <type>material</type>
  <object-name>Face</object-name>
  <object-name>Needle1</object-name>
  <emission>
    <factor-prop>/controls/lighting/instruments-norm</factor-prop>
    <red>0.35</red>
    <green>0.35</green>
    <blue>0.4</blue>
  </emission>
 </animation>

 <animation>
  <type>material</type>
  <object-name>bezeloutside</object-name>
  <object-name>bezelBase</object-name>
  <emission>
    <factor-prop>/controls/lighting/cabin-norm</factor-prop>
    <red>0.2</red>
    <green>0.1</green>
    <blue>0.1</blue>
  </emission>
 </animation>

 <animation>
  <type>rotate</type>
  <object-name>Needle1</object-name>
  <property>/systems/vacuum/suction-inhg</property>
  <min>0</min>
  <max>10</max>
  <interpolation>
   <entry>
    <ind>0</ind>
    <dep>0</dep>
   </entry>
   <entry>
    <ind>10</ind>
    <dep>300</dep>
   </entry>
  </interpolation>
  <axis>
   <x>-1</x>
   <y>0</y>
   <z>0</z>
  </axis>
 </animation>

</PropertyList>
