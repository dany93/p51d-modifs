<?xml version="1.0"?>
<PropertyList>

 <path>blinker.ac</path>

 <animation>
  <type>rotate</type>
  <object-name>blinker</object-name>
  <property>/fdm/jsbsim/systems/Ox/blinker</property>
  <interpolation>
    <entry>
    <ind>0</ind>
    <dep>00</dep>
   </entry>
   <entry>
    <ind>1</ind>
    <dep>49</dep>
   </entry>
  </interpolation>
   <center>
   <x-m>-0.029</x-m>
   <y-m>0.0</y-m>
   <z-m>0.0</z-m>
  </center>
  <axis>
   <x>0</x>
   <y>0</y>
   <z>1</z>
  </axis>
 </animation>

 <animation>
  <type>material</type>
  <object-name>Face</object-name>
  <object-name>blinker</object-name>
  <emission>
    <factor-prop>/controls/lighting/instruments-norm</factor-prop>
    <red>0.35</red>
    <green>0.35</green>
    <blue>0.4</blue>
  </emission>
 </animation>

 <animation>
  <type>material</type>
  <object-name>bezeloutside</object-name>
  <object-name>bezelBase</object-name>
  <emission>
    <factor-prop>/controls/lighting/cabin-norm</factor-prop>
    <red>0.2</red>
    <green>0.1</green>
    <blue>0.1</blue>
  </emission>
 </animation>

</PropertyList>
