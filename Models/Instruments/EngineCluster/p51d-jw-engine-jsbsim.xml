<?xml version="1.0"?>
<PropertyList>

 <path>p51d-jw-engine.ac</path>

 <animation>
  <type>rotate</type>
  <object-name>NeedleOilTemp</object-name>
  <property>/fdm/jsbsim/systems/engine/oil-temp-degC</property>
  <min>0</min>
  <max>100</max>
  <interpolation>
   <entry>
    <ind>0</ind>
    <dep>-30</dep>
   </entry>
   <entry>
    <ind>100</ind>
    <dep>123</dep>
   </entry>
  </interpolation>
  <center>
   <x-m>0</x-m>
   <y-m>0.0</y-m>
   <z-m>0.0</z-m>
  </center>
  <axis>
   <x>-1</x>
   <y>0</y>
   <z>0</z>
  </axis>
 </animation>

 <animation>
  <type>rotate</type>
  <object-name>NeedleOilPress</object-name>
  <property>/fdm/jsbsim/systems/engine/oil-pressure-psi</property>
  <min>0</min>
  <max>200</max>
  <interpolation>
   <entry>
    <ind>0</ind>
    <dep>0</dep>
   </entry>
   <entry>
    <ind>200</ind>
    <dep>190</dep>
   </entry>
  </interpolation>
  <center>
   <x-m>0</x-m>
   <y-m>-0.0087</y-m>
   <z-m>-0.0116</z-m>
  </center>
  <axis>
   <x>-1</x>
   <y>0</y>
   <z>0</z>
  </axis>
 </animation>

 <animation>
  <type>rotate</type>
  <object-name>NeedleFuel</object-name>
  <property>/fdm/jsbsim/systems/engine/fuel-pump-psi</property>
  <min>0</min>
  <max>25</max>
  <interpolation>
   <entry>
    <ind>0</ind>
    <dep>0</dep>
   </entry>
   <entry>
    <ind>12</ind>
    <dep>92</dep>
   </entry>
   <entry>
    <ind>25</ind>
    <dep>180</dep>
   </entry>
  </interpolation>
  <center>
   <x-m>0</x-m>
   <y-m>0.0087</y-m>
   <z-m>-0.0116</z-m>
  </center>
  <axis>
   <x>1</x>
   <y>0</y>
   <z>0</z>
  </axis>
 </animation>
 

 <animation>
  <type>material</type>
  <object-name>Face</object-name>
  <object-name>NeedleOilTemp</object-name>
  <object-name>NeedleOilPress</object-name>
  <object-name>NeedleFuel</object-name>
  <emission>
    <factor-prop>controls/lighting/instruments-norm</factor-prop>
    <red>0.35</red>
    <green>0.35</green>
    <blue>0.4</blue>
  </emission>
 </animation>

 <animation>
  <type>material</type>
  <object-name>bezeloutside</object-name>
  <object-name>bezelBase</object-name>
  <emission>
    <factor-prop>/controls/lighting/cabin-norm</factor-prop>
    <red>0.2</red>
    <green>0.1</green>
    <blue>0.1</blue>
  </emission>
 </animation>

</PropertyList>
