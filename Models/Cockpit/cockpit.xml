        <min-m>0</min-m>
        <max-m>1000</max-m>
    </animation>    
   
 <animation>
  <type>rotate</type>
  <object-name>ReostatKnob</object-name>
  <property>controls/lighting/cabin-norm</property>
  <factor>270</factor>
  <axis>   
   <x1-m>2.5875</x1-m>
