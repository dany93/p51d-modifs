 <animation>
    <type>pick</type>
    <object-name>gearLeverForward</object-name>
    <action>
    <button>0</button>
    <binding>
        <command>nasal</command>
        <script>
            if (getprop("/controls/gear/gear-down") == 0)
            {
                setprop("/controls/gear/gear-down", 1);
            }
            else
            {
                setprop("/controls/gear/gear-down", 0);
            }
        </script>
    </binding>
   </action>
  </animation>

  <animation>
    <type>rotate</type>
      <object-name>gearLeverForward</object-name>
      <object-name>gearLeverHardware</object-name>
    <property>/controls/gear/leverPos</property>
    <interpolation>
    <entry>
        <ind>0.0</ind>
        <dep>0</dep>
    </entry>
    <entry>
        <ind>0.05</ind>
        <dep>5</dep>
    </entry>
    <entry>
        <ind>0.95</ind>
        <dep>5</dep>
    </entry>
    <entry>
        <ind>1.0</ind>
        <dep>0</dep>
    </entry>
    </interpolation>
    <center>
        <x-m>2.8129</x-m>
        <y-m>-0.2662</y-m>
        <z-m>-0.2437</z-m>
    </center>
    <axis>
        <x>0.0</x>
        <y>0.0</y>
        <z>-1.0</z>
    </axis>
  </animation>

  <animation>
    <type>rotate</type>    
    <object-name>gearLeverForward</object-name>
    <object-name>gearLeverHardware</object-name>
    <object-name>gearLeverRear</object-name>
    <property>/controls/gear/leverPos</property>
    <factor>38</factor>
    <center>
        <x-m>2.8769</x-m>
        <y-m>-0.287185</y-m>
        <z-m>-0.26492</z-m>
    </center>
    <axis>
        <x>0.0</x>
        <y>1.0</y>
        <z>0.0</z>
    </axis>
  </animation>
  
  
