<?xml version="1.0"?>
<PropertyList>

 <path>flaps-control.ac</path>

 <animation>
  <type>rotate</type>
  <object-name>flaps-lever</object-name>
      <property>/controls/flight/flaps</property>
  <interpolation>
   <entry>
    <ind>0</ind>
    <dep>0</dep>
   </entry>
   <entry>
    <ind>1.0</ind>
    <dep>-55</dep>
   </entry>
  </interpolation>
  <axis>
   <x>0</x>
   <y>-1</y>
   <z>0</z>
  </axis>
 </animation>

 <animation>
  <type>rotate</type>
  <object-name>flaps-knob</object-name>
      <property>/controls/flight/flaps</property>
  <interpolation>
   <entry>
    <ind>0</ind>
    <dep>0</dep>
   </entry>
   <entry>
    <ind>1.0</ind>
    <dep>-55</dep>
   </entry>
  </interpolation>
  <axis>
   <x>0</x>
   <y>-1</y>
   <z>0</z>
  </axis>
 </animation>

  <animation>
  <type>material</type>
  <object-name>flaps-index</object-name>
  <emission>
    <factor-prop>controls/lighting/cabin-norm</factor-prop>
    <red>0.25</red>
    <green>0.15</green>
    <blue>0.15</blue>
  </emission>
 </animation>

 <animation>
  <type>material</type>
  <object-name>flaps-knob</object-name>
  <object-name>flaps-lever</object-name>
  <emission>
    <factor-prop>controls/lighting/cabin-norm</factor-prop>
    <red>0.15</red>
    <green>0.0</green>
    <blue>0.0</blue>
  </emission>
 </animation>

</PropertyList>